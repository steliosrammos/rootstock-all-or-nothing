{
  "name": "aon-cli",
  "version": "1.0.0",
  "description": "CLI tool for interacting with AON (All-Or-Nothing) crowdfunding contracts",
  "main": "dist/index.js",
  "bin": {
    "aon-cli": "dist/index.js"
  },
  "scripts": {
    "build": "npm run clean && tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "clean": "rimraf dist",
    "prepare": "npm run build",
    "generate:abis": "wagmi generate",
    "cli": "npm run build && node dist/index.js",
    "watch": "tsc --watch",
    "type-check": "tsc --noEmit",
    "link:global": "npm run build && npm link",
    "unlink:global": "npm unlink -g aon-cli",
    "relink:global": "npm run unlink:global && npm run link:global"
  },
  "keywords": [
    "ethereum",
    "crowdfunding",
    "blockchain",
    "cli",
    "rootstock",
    "rsk"
  ],
  "author": "Stelios Rammos",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "inquirer": "^8.2.6",
    "ora": "^5.4.1",
    "table": "^6.8.1",
    "viem": "^2.21.1",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/inquirer": "^8.2.10",
    "@types/node": "^20.8.7",
    "@wagmi/cli": "^2.1.15",
    "@wagmi/core": "^2.13.4",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
